{
  "name": "sample-analysis",
  "data_source_config": {
    "type": "custom",
    "item_schema": {
      "type": "object",
      "properties": {
        "input": { "type": "string" },
        "expected": { "type": "string" }
      },
      "required": ["input", "expected"]
    },
    "include_sample_schema": true
  },
  "testing_criteria": [
    {
      "type": "string_check",
      "name": "Match output to expected answer",
      "input": "{{ sample.output_json }}",
      "operation": "eq",
      "reference": "{{ item.expected }}"
    }
  ]
}
