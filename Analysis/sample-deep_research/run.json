{
  "name": "Sample Analysis Run",
  "data_source": {
    "type": "completions",
    "model": "gpt-4.1",
    "input_messages": {
      "type": "template"
    }
  },
  "sampling_params": {
    "temperature": 0,
    "max_tokens": 1000,
    "text": {
      "format": {
        "type": "json_schema",
        "name": "analysis_output",
        "schema": {
          "type": "object",
          "properties": {
            "score": {
              "type": "number",
              "description": "Rating from 0 (terrible) to 5 (great)"
            },
            "analysis": {
              "type": "string",
              "description": "Detailed financial analysis"
            }
          },
          "required": ["score", "analysis"],
          "additionalProperties": false
        },
        "strict": true
      }
    }
  }
}
